#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <utility>

using namespace std;

struct Student {
    string name1, name2;
    int d, m, y;
};

bool stsrt_name(Student& a, Student& b) {
    if (a.name2 != b.name2)
        return a.name2 < b.name2;
    if (a.name1 != b.name1)
        return a.name1 < b.name1;
    if (a.y != b.y)
        return a.y < b.y;
    if (a.m != b.m)
        return a.m < b.m;
    return a.d < b.d;
}

bool stsrt_date(Student& a, Student& b) {
    if (a.y != b.y)
        return a.y < b.y;
    if (a.m != b.m)
        return a.m < b.m;
    if (a.d != b.d)
        return a.d < b.d;
    if (a.name2 != b.name2)
        return a.name2 < b.name2;
    return a.name1 < b.name1;
}

int main() {
    int n, d, m, y;
    string name1, name2;
    cin >> n;
    vector<Student> a(n);
    for (int i = 0; i < n; i++) {
        cin >> name1 >> name2 >> d >> m >> y;
        a[i].name1 = name1;
        a[i].name2 = name2;
        a[i].d = d;
        a[i].m = m;
        a[i].y = y;
    }
    string q;
    cin >> q;
    if (q == "name")
        sort(begin(a), end(a), stsrt_name);
    else
        sort(begin(a), end(a), stsrt_date);
    for (int i = 0; i < n; i++) {
        cout << a[i].name1 << ' ' << a[i].name2
            << ' ' << a[i].d << '.' << a[i].m << '.' << a[i].y << endl;
    }
}
